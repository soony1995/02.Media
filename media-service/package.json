{
  "name": "media-service",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "migrate": "npm run build && node dist/scripts/migrate.js",
    "migrate:prod": "node dist/scripts/migrate.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.2",
    "kysely": "^0.27.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.13.1",
    "jsonwebtoken": "^9.0.2",
    "sharp": "^0.33.5",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/pg": "^8.10.2",
    "@types/cors": "^2.8.17",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
